{
  "name": "shintyoku-timer",
  "version": "0.0.1",
  "description": "",
  "main": "dist/main/index.js",
  "scripts": {
    "build": "npm run prepare && webpack && npm run build:scss && npm run build:font && flow check",
    "build:scss": "node-sass src/renderer/index.scss -o dist/renderer/  --include-path node_modules",
    "build:font": "mkdir -p ./dist/fonts && cp ./node_modules/bootstrap/fonts/* ./dist/fonts",
    "watch": "npm run prepare && concurrently --kill-others \"webpack -w\" \"npm run watch:scss\"",
    "watch:scss": "node-sass src/renderer/index.scss -o dist/renderer/  --include-path node_module -w",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "prepare": "mkdir -p dist/renderer/ && cp src/renderer/index.html dist/renderer/ && npm run build:font",
    "flow": "flow; test $? -eq 0 -o $? -eq 2"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^3.3.6",
    "electron-prebuilt": "^0.37.8",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-saga": "^0.10.4"
  },
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.9.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "coffee-script": "^1.10.0",
    "concurrently": "^2.1.0",
    "css-loader": "^0.23.1",
    "flow-bin": "^0.26.0",
    "flow-typed": "^2.0.0-beta.4",
    "install": "^0.8.1",
    "node-sass": "^3.7.0",
    "npm": "^3.9.3",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0"
  }
}
